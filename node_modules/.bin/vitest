#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/mota1/Downloads/encuestas-sri (1)/encuestas-sri/node_modules/.pnpm/vitest@3.2.4_@types+node@22_0d86aa73b619eb7941abb770cdd0d6fc/node_modules/vitest/node_modules:/mnt/c/Users/mota1/Downloads/encuestas-sri (1)/encuestas-sri/node_modules/.pnpm/vitest@3.2.4_@types+node@22_0d86aa73b619eb7941abb770cdd0d6fc/node_modules:/mnt/c/Users/mota1/Downloads/encuestas-sri (1)/encuestas-sri/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/mota1/Downloads/encuestas-sri (1)/encuestas-sri/node_modules/.pnpm/vitest@3.2.4_@types+node@22_0d86aa73b619eb7941abb770cdd0d6fc/node_modules/vitest/node_modules:/mnt/c/Users/mota1/Downloads/encuestas-sri (1)/encuestas-sri/node_modules/.pnpm/vitest@3.2.4_@types+node@22_0d86aa73b619eb7941abb770cdd0d6fc/node_modules:/mnt/c/Users/mota1/Downloads/encuestas-sri (1)/encuestas-sri/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
